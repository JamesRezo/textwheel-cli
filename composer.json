{
    "name": "jamesrezo/textwheel",
    "description": "TextWheel CLI Application",
    "type": "library",
    "repositories": [{
        "type": "git",
        "url": "https://github.com/JamesRezo/textwheel.git"
    }],
    "require": {
        "symfony/console": "^2.8 || ^3.1",
        "textwheel/engine": "dev-php-library"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "JamesRezo",
            "email": "james@rezo.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "TextWheel\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TextWheel\\Test\\Cli\\": "tests/"
        }
    },
    "bin": ["bin/textwheel"]
}
